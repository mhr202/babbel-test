{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "react-app/jest"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [2, "single", { "avoidEscape": true }],
        "react/function-component-definition": [
            2,
            {
              "namedComponents": "arrow-function"
            }
          ]
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
    
      
}
